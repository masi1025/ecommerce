<?xml version="1.0" encoding="UTF-8"?>
<!-- https://spotbugs.readthedocs.io/en/stable/filter.html#complete-example -->
<!-- https://spotbugs.readthedocs.io/en/stable/filter.html#introduction-to-filter-files -->
<!-- https://github.com/spotbugs/spotbugs/blob/master/spotbugs/etc/findbugsfilter.xsd -->
<!-- https://raw.githubusercontent.com/spotbugs/spotbugs/4.8.4/spotbugs/etc/findbugsfilter.xsd -->
<FindBugsFilter
    xmlns="https://github.com/spotbugs/filter/4.8.4"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/spotbugs/filter/4.8.4 https://raw.githubusercontent.com/spotbugs/spotbugs/4.8.4/spotbugs/etc/findbugsfilter.xsd">
    <Match>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD"/>
    </Match>
    <Match>
        <!-- https://spotbugs.readthedocs.io/en/stable/filter.html#class -->
        <Class name="~.*\.*MapperImpl"/>
    </Match>
</FindBugsFilter>
